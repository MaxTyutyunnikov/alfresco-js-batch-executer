<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>

	<bean id="transactionScript" parent="baseJavaScriptExtension" class="com.ciber.alfresco.repo.jscript.ScriptTransactionService">
		<property name="extensionName" value="transactionService"/>
		<property name="transactionService" ref="TransactionService"/>
	</bean>

	<bean id="batchesScript" parent="baseJavaScriptExtension" class="com.ciber.alfresco.repo.jscript.batchexecuter.ScriptBatchExecuter">
		<property name="extensionName" value="batchExecuter"/>
		<property name="serviceRegistry" ref="ServiceRegistry"/>
	</bean>

</beans>
